# generate js for browser testing
# no auto-execution
-cp src/main
-cp src/test
-lib monax
-main TestAll
-cp ./lib/utest/src
-js bin/test.js

--next

-cp src/main
-cp src/test
-lib monax
-lib nodejs
-main TestAll
-cp ./lib/utest/src
-D nodejs
-js bin/test_node.js
-cmd echo "--------------------------------------NODE--------------------------------------"
-cmd node bin/test_node.js

--next

# display completions
-cp src/main
-cp src/test
-lib monax
-main TestAllQueue
-cp ./lib/utest/src
-cmd echo "--------------------------------------NEKO--------------------------------------"
-x bin/test

--next


-cp src/main
-cp src/test
-lib monax
-main TestAllQueue
-cp ./lib/utest/src
-cpp bin/testcpp
-cmd echo "--------------------------------------CPP---------------------------------------"
-cmd ./bin/testcpp/TestAllQueue

--next

-cp src/main
-cp src/test
-lib monax
-main TestAllQueue
-cp ./lib/utest/src
-java bin/testjava
-cmd echo "--------------------------------------JAVA--------------------------------------"
-cmd java -jar bin/testjava/testjava.jar


